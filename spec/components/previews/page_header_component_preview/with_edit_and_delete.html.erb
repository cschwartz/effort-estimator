<%
  project = Project.new(id: 1, title: "Sample Project")
  project.define_singleton_method(:persisted?) { true }
%>
<%= render PageHeaderComponent.new(project) do |c| %>
  <% c.with_action do %>
    <%= render EditActionComponent.new(href: "#", turbo_frame: "edit_project") %>
  <% end %>
  <% c.with_action do %>
    <%= render DeleteActionComponent.new(href: "#") %>
  <% end %>
<% end %>
